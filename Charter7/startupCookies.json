[
  {
    "description":"get-cookies请求",
    "request":{
      "uri":"/getcookies",
      "method":"get",
      "cookies":{
        "login":"login"
      }
      },
    "response":{
      "text":"get-cookies方法返回"
    }
  },

  {
    "description":"post-cookies请求",
    "request":{
      "uri":"/postcookies",
      "method":"post",
      "cookies":{
        "login":"login"
      },
      "json":{
        "name":"zhangsan",
        "age":"18"
      }
    },
    "response":{
      "json":{
        "message":"success",
        "code":"200"
      },
      "status":200
    }
  },

  {
    "description":"header请求",
    "request":{
      "uri":"/header",
      "method":"post",
      "headers":{
        "content-type":"application/json"
      },
      "json":{
        "name":"zhangsan",
        "sex":"man"
      }
    },
    "response":{
      "json":{
        "message":"success",
        "code":200
      },
      "status":200
    }
  },

  {
    "description":"返回cookies信息的请求",
    "request":{
      "uri":"/getCookies",
      "method":"get"
    },
    "response":{
      "cookies":{
        "login":"login"
      },
      "text":"获得cookies信息成功"
    }
  }
]